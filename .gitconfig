[core]
    excludesfile = ~/.gitignore
	editor = emacs -nw
    autocrlf = false
    ignorecase = false
    quotepath = false
[merge]
    ff = false
[pull]
    ff = only
[help]
    autocorrect = 10
[color]
    ui = true
[alias]
    graph = log --graph --color --pretty=format:\"%C(yellow)%H%C(green)%d%C(reset)%n%x20%cd%n%x20%cn%x20(%ce)%n%x20%s%n\"
    st = status -uall
    ck = checkout
    cm = commit --signoff
[credential]
    helper = cache --timeout=86400
[pager]
	log = less
[init]
	defaultBranch = main
